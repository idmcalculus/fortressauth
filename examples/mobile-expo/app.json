{
  "expo": {
    "name": "FortressAuth Expo",
    "slug": "fortressauth-mobile-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "fortressauth",
    "splash": {
      "backgroundColor": "#1e3a5f"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1e3a5f"
      }
    },
    "plugins": ["expo-router", "expo-secure-store"]
  }
}
