{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**",
        ".svelte-kit/**",
        "out/**"
      ]
    },
    "test": {
      "dependsOn": [
        "build"
      ]
    },
    "test:coverage": {
      "dependsOn": [
        "build"
      ]
    },
    "lint": {},
    "lint:fix": {},
    "typecheck": {
      "dependsOn": [
        "^build"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}